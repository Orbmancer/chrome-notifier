{
  "manifest_version": 2,

  "name": "Chrome Notifier",
  "description": "Let chrome notify you when changes happen on elements you track",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
   {
    "matches": ["http://rundeck.iadvize.net:4440/project/Deploy/execution/show/*"],
    "js": ["jquery-3.1.0.min.js", "observer.js"]
   }
 ],
  "permissions": [
    "notifications",
    "background"
  ]
}
